<?xml version="1.0" encoding="ISO-8859-1"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  <xsl:template match="/">
    <html>
      <head>
        <title><xsl:value-of select="/feed/title" /></title>

        <style type="text/css">
        </style>
        
      </head>
      
      <body>
        <div align="center">
          <div id="headerInfo">
            <h1><xsl:value-of select="/feed/title" /></h1>
            <p align="center" id="headerText">This page is the syndication feed for 
            <b><xsl:value-of select="/feed/title" /></b>.  You can subscribe
            to this feed using an aggregator program.</p>
          </div>
        </div>
      </body>
    </html>
  </xsl:template>
</xsl:stylesheet>
